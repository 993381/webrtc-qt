1. query camera record
client-->camera
{
    "type":"tunnel",
    "command":"query_record",
    "condition":{
        "startTime":"2014/04/17 17:51:00", //query start time
        "endTime":"2014/04/18 17:55:00",   //query stop time
        "offset":0,
        "toQuery":30
    }
}

camera-->client
{
    "type":"tunnel",
    "command":"query_record",
    "condition":{
        "startTime":"2014/04/17 17:51:00", //query start time
        "endTime":"2014/04/18 17:55:00",   //query stop time
        "offset":0,
        "toQuery":30
    },
    "totalNum":100,    //the total records from startime to endtime
    "recordList":[{"fileName":"",//the record dir stored in camera
            "fileEndTime":"2014/04/17 17:51:00",//the record file start  time
            "fileSize":256,//record file size
     }]
}

2.ptz control message
client-->camera
{
    "type":"tunnel",
    "command":"ptz",
    "control":"move_left",//this could be "move_left","move_right","move_up","move_down","stop"
          // and others
    "param":10 //the param need to send to camera,

    //for the move control this param is not useful
}

3. p2p connect error

camera-->client
{
    "type":"tunnel",
    "command":"p2p_error",
    "errorType":"tunnel_max" // tunnel_max 设备最大连接数,tunnel_replace 此链接被其他链接替换
}

3. get camera wifi status

client-->camera
{
    "type":"tunnel",
    "command":"wifi_status"
}
camera-->client
{
    "type":"tunnel",
    "command":"wifi_status",
    "status":4 //wifi连接的状态，0-未开始+未使能(插着网线同时wifi未设置)，1-未开始+已使能(wifi参数已经设置但插着网线)，2-开始+未使能(网线拔掉了但wifi未设置)，3-开始连接或者正在连接，4-连接成功
}

4.get camera wifi info
client-->camera
{
    "type":"tunnel",
    "command":"wifi_info"
}
camera-->client
{
    "type":"tunnel",
    "command":"wifi_info",
    "wifis":[{
        "ssid":"", //the ssid of wifi
        "signalStrength":0,//信号强度
        "encryptMode":1,//加密方式
        "encryptFormat":1,//加密格式
        "enable":0//正在使用 1,未使用 0
    }]
}

5. set camera wifi
client-->camera
{
    "type":"tunnel",
    "command":"set_wifi",
    "param":{
        "ssid":"",  //ssid of wifi
        "key":"",  //password of wifi
        "enable":0,  //enable wifi 1,disable wifi 0
        "encryptMode":0,  //0-none auth,1-wep ,2-wpa,3-wpa2
        "encryptFormat":0,  //0-enc tkip,1-enc aes
    }
}
camera-->clent
{
    "type":"tunnel",
    "command":"set_wifi",
    "result":1,//0 false,1 success
}

6. check online
camera<-->clent
{
    "type":"tunnel",
    "command":"echo"
}

7. camera restart
client-->camera
{
    "type":"tunnel",
    "command":"reboot"
}
client<--camera
{
    "type":"tunnel",
    "command":"reboot",
    "result":0
}

8. get or set cmaera alarm status
client<-->camera
{
    "type":"tunnel",
    "command":"arming_status",
    "value":1,    //0:disable alarm,1:enabble status；
                //无此字段:获取报警状态
}


9. set camera name
client-->camera
{
    "type":"tunnel",
    "command":"rename",
    "name":"camear name",
}
client<--camera
{
    "type":"tunnel",
    "command":"rename",
    "result":0,
}

