3. get camera wifi status

client-->camera
{
    "type":"tunnel",
    "command":"wifi_status"
}
camera-->client
{
    "type":"tunnel",
    "command":"wifi_status",
    "status":""
}

4.get camera wifi info
client-->camera
{
    "type":"tunnel",
    "command":"wifi_info"
}
camera-->client
{
    "type":"tunnel",
    "command":"wifi_info",
    "wifis":[{
        "ssid":"", //the ssid of wifi
        "signalStrength":0,//信号强度
        "encryptMode":1,//加密方式
        "encryptFormat":1,//加密格式
        "enable":0//正在使用 1,未使用 0
    }]
}

5. set camera wifi
client-->camera
{
    "type":"tunnel",
    "command":"set_wifi",
    "param":{
        "ssid":"",  //ssid of wifi
        "key":"",  //password of wifi
        "enable":0,  //enable wifi 1,disable wifi 0
        "encryptMode":0,  //0-none auth,1-wep ,2-wpa,3-wpa2
        "encryptFormat":0,  //0-enc tkip,1-enc aes
    }
}
camera-->clent
{
    "type":"tunnel",
    "command":"set_wifi",
    "result":1,//0 false,1 success
}

6. check online
camera<-->clent
{
    "type":"tunnel",
    "command":"echo"
}

7. camera restart
client-->camera
{
    "type":"tunnel",
    "command":"restart"
}
client<--camera
{
    "type":"tunnel",
    "command":"restart",
    "result":0
}

8. get or set cmaera alarm status
client<-->camera
{
    "type":"tunnel",
    "command":"arming_status",
    "value":1,    //0:disable alarm,1:enabble status；
                //无此字段:获取报警状态
}


9. set camera name
client-->camera
{
    "type":"tunnel",
    "command":"rename",
    "name":"camear name",
}
client<--camera
{
    "type":"tunnel",
    "command":"rename",
    "result":0,
}

